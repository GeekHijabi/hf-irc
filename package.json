{
  "name": "halal-foods-irc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:server": "babel ./server -d ./server/dist --copy-files",
    "build:client": "webpack --config webpack.config.js",
    "postinstall": "npm run build",
    "start": "npm run build && nodemon server/dist/app.js",
    "client:dev": "webpack-dev-server --hot",
    "start:dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --watch server --exec babel-node server/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "sequelize db:seed:all",
    "migrate": "sequelize db:migrate",
    "undo": "sequelize db:migrate:undo:all",
    "reset": "npm run undo && npm run migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
